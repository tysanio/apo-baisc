stock IsAGang(playerid)
{
	switch(PlayerInfo[playerid][pMember])
	{
		case FRACTION_BALLAS, FRACTION_VAGOS, FRACTION_GROVE, FRACTION_AZTEC, FRACTION_RIFA: return 1;
	}

	return false;
}


