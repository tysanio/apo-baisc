stock ShowStats(playerid, params)
{
	if(PlayerInfo[params][pLogin] == 0) return 1;
	new str[324];

	new atext[20], jtext[229];
	if(PlayerInfo[params][pSex] == 1) atext = "Laki-laki", jtext = "Menikah dengan:\t\t\t-\n";
	else atext = "Perempuan", jtext = "Menikah dengan:\t\t\t-\n";

	new htext[32], h_id = GetPlayerHouse(params);
	/*if(GetPlayerHouse(params) != -1) htext = "Tuna wisma";
	else format(htext, sizeof(htext), "Nomor rumah%d", GetPlayerHouse(params));*/
	if(h_id != -1) format(htext, sizeof(htext), "Nomor rumah%d ($%i)", h_id, HouseInfo[h_id][hValue]);
	else if(PlayerInfo[playerid][pHotel] != -1) format(htext, sizeof(htext), "No. hotel%d", PlayerInfo[playerid][pHotel]);
	else htext = "Tuna wisma";

	new btext[64], b_id = GetPlayerBizz(params);
	if(b_id == -1) btext = "Tidak";
	else format(btext, sizeof(btext), "%s ($%i)", BizzInfo[b_id][bMessage], BizzInfo[b_id][bMoney]);

	new azctext[64], a_id = GetPlayerABizz(params);
	if(a_id == 0) azctext = "Tidak";
	else format(azctext, sizeof(azctext), "%s ($%i)", BizzAInfo[a_id][aMessage], BizzAInfo[a_id][aMoney]);

	new drank[20];
	switch(PlayerInfo[params][pLevel])
	{
 		case 1..2: drank = "Pemula";
 		case 3..5: drank = "Pemula";
	 	case 6..11: drank = "Tentu";
	 	case 12..15: drank = "Pemain reguler";
	 	case 16..20: drank = "Dikuasai";
	 	case 21..25: drank = "Kakek";
	 	default: drank = "Veteran";
	}

	if(PlayerInfo[params][pLeader]) drank = "Pemimpin";
	if(admin_level[params]) drank = "Admin";

	new infofrac[32];
	if(PlayerInfo[params][pMember] == 0) infofrac = "-";
	else format(infofrac, sizeof(infofrac), "%s", FracInfo[PlayerInfo[params][pMember]][fName]);

	new infojob[64];
	if(PlayerInfo[params][pJob] == 0 && PlayerInfo[params][pMember] == 0) infojob = "Menganggur";
	else if(PlayerInfo[params][pJob] != 0) format(infojob, sizeof(infojob), "%s", NamesJob(params));
	else if(PlayerInfo[params][pMember] != 0) format(infojob, sizeof(infojob), "%s", FracRank[PlayerInfo[params][pMember]][PlayerInfo[params][pRank]]);

	String = "";
	format(str, sizeof(str), "\
	{ffffff}Nama depan:\t\t\t\t{1A8FA9}%s\n\
	"W"Level:\t\t\t%d (%d dari %d)\n\
	Uang ada di tangan:\t\t%d$\n\
	Uang di bank:\t\t%d$\n\
	Nomor telepon:\t\t%d\n\
	Di telepon:\t\t%d$\n\
	Ketaatan hukum:\t\t%d\n\
	Tingkat yang diinginkan:\t\t%d\n\
	Obat-obatan:\t\t\t%d gr\n\
	Kartrid:\t\t\t%d pcs\n\
	Logam:\t\t\t%d kg\n\
	Gender:\t\t\t\t%s\n\
	Kecanduan:\t\t\t%i\n",
	Name(params), PlayerInfo[params][pLevel], PlayerInfo[params][pExp], (PlayerInfo[params][pLevel]+1)*4, PlayerInfo[params][pCash], PlayerInfo[params][pBank], PlayerInfo[params][pNumber],
	PlayerInfo[params][pSummaNumber], PlayerInfo[params][pZakon], PlayerInfo[params][pWanted], PlayerInfo[params][pDrugs],
	PlayerInfo[params][pPatron], PlayerInfo[params][pMetall], atext, PlayerInfo[playerid][pDrugDep]);
	strcat(String,str);

 	format(str, sizeof(str), "\
	%s\n\
	Organisasi:\t\t\t%s\n\
	Pekerjaan / posisi:\t\t%s\n\n\
	Akomodasi:\t\t\t%s\n\
	Perusahaan:\t\t\t%s\n\
    SPBU:\t\t\t\t%s\n\n\
    Status Game:\t\t%s\n\
	Peringatan:\t\t%d dari 3\n\
	Status VIP:\t\t\t%s\n\
	Menang kembali AFK: %s\n",
 		jtext, infofrac, infojob, htext, btext, azctext, drank, PlayerInfo[params][pWarn], VipNames(params),
 		ConvertSeconds(PlayerInfo[params][pInGameSeconds] - PlayerInfo[params][pInGameAFKSeconds]));

	strcat(String,str);

	if(admin_level[playerid])
	{
	    strcat(String, "\nTransportasi:\n\n");

		for(new i; i < MAX_PLAYER_VEHICLES; i++)
		{
		    new model = CarInfo[params][cModel][i];
		    new price = CarInfo[params][cCost][i];

		    if(model == 0)
		    {
		        format(str, 48, "%i. Kosong\n", i + 1);
			    strcat(String, str);
		    }
		    else
			{
				format(str, 48, "%i. %s ($%i)\n", i + 1, VehicleNames[model - 400], price);
			    strcat(String, str);
			}
		}

     	format(str, 324,
	        "\nDimainkan dalam sehari: %s\n\
			AFK per hari: %s\n\
			Menang dalam satu jam terakhir: %s\n\n\
			Perangkat: %s",
			ConvertSeconds(PlayerInfo[params][pInGameSeconds]),
			ConvertSeconds(PlayerInfo[params][pInGameAFKSeconds]),
			ConvertSeconds(PlayerInfo[params][pInGameHourSeconds]),
			IsPlayerMobile(params) ? ("Android") : ("PC"));

		strcat(String ,str);
	}

	SPD(playerid,0,DIALOG_STYLE_MSGBOX,"{E5D81E}Statistik pemain",String, "Tutup", "");
	return 1;
}

public OnPlayerConnect(playerid)
{
  
    // Checking for plugin availability
    if (SvGetVersion(playerid) == SV_NULL)
    {
        SendClientMessage(playerid, -1, "Could not find plugin sampvoice.");
    }
    // Checking for a microphone
    else if (SvHasMicro(playerid) == SV_FALSE)
    {
        SendClientMessage(playerid, -1, "The microphone could not be found.");
    }
    // Create a local stream with an audibility distance of 40.0, an unlimited number of listeners
    // and the name 'Local' (the name 'Local' will be displayed in red in the players' speakerlist)
    else if ((lstream[playerid] = SvCreateDLStreamAtPlayer(40.0, SV_INFINITY, playerid, 0xff0000ff, "Local")))
    {
        SendClientMessage(playerid, -1, "Press Z to talk to global chat and B to talk to local chat.");

        // Attach the player to the global stream as a listener
        if (gstream) SvAttachListenerToStream(gstream, playerid);

        // Assign microphone activation keys to the player
        SvAddKey(playerid, 0x42);
        SvAddKey(playerid, 0x5A);
    }

    PhoneModeInit(playerid);
	PhoneShow[playerid] = false;
	PhoneLocked[playerid] = 0;
	PlayerTent[playerid] = 0;


	PlayerInfo[playerid][Neon] = 0;
	VerificaNeon[playerid] = 0;

	SedangHauling[playerid] = 0;
	//========strobe============
	new VID = GetPlayerVehicleID(playerid);
	RemoveVehSS(VID);

    player_admin_sms[playerid] = false;

	PlayerInfo[playerid][pMember] = 0;

    PlayerInfo[playerid][pSkin] = -1;

    ClearPlayerOffer(playerid);

    family_player_3d[playerid] = Text3D:-1;

    player_news_live[playerid] = false;

    PlayerInfo[playerid][pSupport] = 0;

	for(new i; i < MAX_PLAYER_VEHICLES; i++) CarInfo[playerid][cModel][i] = 0;

    admin_level[playerid] = 0;

    beg_quest_info[playerid][b_quest_id] = 0;
    beg_quest_info[playerid][b_quest_status] = 0;
    beg_quest_info[playerid][b_quest_progress] = 0;

    StartFarm[playerid] = 0;

    player_green_hits[playerid] = 0;

	if(bonus_exp) SCM(playerid, 0xa86cfcFF, "Perhatian! {FFFFFF}Server menyertakan promosi x2 EXP di PayDay!");
    if(bonus_donate) SCM(playerid, 0xa86cfcFF, "Perhatian! {FFFFFF}Di server, pengisian akun promosi x2 disertakan!");

    player_bag[playerid] = false;
    player_gag[playerid] = false;
    player_tie[playerid] = false;

    new hour, minute, second;
	gettime(hour, minute, second);

	GetPlayerIp(playerid, GetPlayerIpEx(playerid), 16);

	SetPlayerTime(playerid, hour, minute);

    player_ship_worked[playerid] = false;

    player_taxi_passanger[playerid] = INVALID_PLAYER_ID;

    follow_id[playerid] = INVALID_PLAYER_ID;

    gSpectateID[playerid] = INVALID_PLAYER_ID;

    pTemp[playerid][uborka_text] = Text3D:INVALID_3DTEXT_ID;

    pTemp[playerid][uborka_timer] = 0;

    player_call_police[playerid] = player_call_hospital[playerid] = player_call_mechanic[playerid] = false;

    player_call_taxi[playerid] = 0;

	SetPlayerWeather(playerid, weather_id);

    as_player_camera[playerid] = 0;

	player_rob_progress[playerid] = 0;

    rob_offer[playerid] = INVALID_PLAYER_ID;

    player_rob_id[playerid] = -1;

	pTemp[playerid][bus_text] = Text3D:INVALID_3DTEXT_ID;

    player_quest_info[playerid][quest_pickup_id] = 0;

    graffiti_player_id[playerid] = -1;

    duel_offer[playerid] = INVALID_PLAYER_ID;

    player_reg_pb[playerid] = -1;

    player_hotel_id[playerid] = -1;

    fa
}

CMD:getstats(playerid, params[])
{
    if(admin_level[playerid] < 1 || !admin_logged[playerid]) return 1;
	if(sscanf(params, "u",params[0])) return Send(playerid, -1, "Masuk: /getstats [id]");

	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, COLOR_GREY, "Pemain belum login!");

	ShowStats(playerid, params[0]);

	return 1;
}
